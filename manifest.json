{
    "name": "feedly_direct",
    "description": "feedlyのfeedsから直接ページにアクセス",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://feedly.com/i/**"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ]
}